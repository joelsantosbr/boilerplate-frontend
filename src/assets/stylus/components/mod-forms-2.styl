#form-mod-2
	padding 80px 0
	border-top 2px solid $customred1
	border-bottom 2px solid $customred1
	background-color $customblack1
	color $customwhite1
	font-size 13px
	font-weight 100
	.page-header
		padding 0 0 25px 0
		strong
			font-size 13px
			font-weight 700
			color $customwhite1
	.contatos
		li
			display table
			position relative
			padding 0 0 15px 0
			vertical-align middle
			span
				display table-cell
				vertical-align middle
				font-size 22px
				padding 0 10px 0 0
	.form
		padding 0 0 15px 0
		.input-group
			margin-bottom 5px
			textarea
				min-height 100px
		.btn
			text-align center
			font-weight 700